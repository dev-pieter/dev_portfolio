"}}>\n                    <a href='https://github.com/dev-pieter' target='_blank' style={{textDecoration: 'none' ,color:'black'}}><i className=\"fa fa-github icon\"></i></a>\n                    <a href='https://www.linkedin.com/in/pieternortje/' target='_blank' style={{textDecoration: 'none' ,color:'black'}}><i className=\"fa fa-linkedin-square icon\"></i></a>\n                </div>\n            </Container>\n        </section>\n    );\n};\n\nexport default Contact;","/Users/pieternortje/Documents/Development/Portfolio2-main/src/App.jsx",["52","53","54"],"import Hero from \"./components/hero.jsx\";\nimport About from \"./components/about.jsx\";\nimport Projects from \"./components/projects/index.jsx\";\nimport Contact from \"./components/contact.jsx\"\nimport History from \"./components/work_history/history.jsx\";\nimport { Link, Element } from 'react-scroll';\nimport './style/general.css';\n\nfunction App() {\n\n  const scrollTo = (ref) => {\n    const scroll = document.getElementById(ref);\n\n    if(scroll){\n      scroll.scrollIntoView({behavior: 'smooth'});\n    }\n  }\n\n\n  return (\n    <div className=\"App\">\n      <div className='nv'>\n                <div>\n                    <h1 className=\"brand\">PN</h1>\n                </div>\n                <div>\n                    <Link className=\"links\" onClick={() => scrollTo('home_scroll')}><i class=\"fa fa-home home-i\" aria-hidden=\"true\"></i>{'Home'}</Link><br/><br/>\n                    <Link className=\"links\" onClick={() => scrollTo('about_scroll')}><i class=\"fa fa-user-circle home-i\" aria-hidden=\"true\"></i>{'About Me'}</Link><br/><br/>\n                    <Link className=\"links\" onClick={() => scrollTo('work_scroll')}><i class=\"fa fa-building home-i\" aria-hidden=\"true\"></i>{'Experience'}</Link><br/><br/>\n                    <Link className=\"links\" onClick={() => scrollTo('projects_scroll')}><i class=\"fa fa-product-hunt home-i\" aria-hidden=\"true\"></i>{'My Work'}</Link><br/><br/>\n                    <Link className=\"links\" onClick={() => scrollTo('contact_scroll')}><i class=\"fa fa-envelope-open home-i\" aria-hidden=\"true\"></i>{'Contact'}</Link>\n                </div>\n                <div>\n                  <a href='https://github.com/dev-pieter' target='_blank' style={{textDecoration: 'none' ,color:'white'}}><i className=\"fa fa-github nav-i\"></i></a>\n                  <a href='https://www.linkedin.com/in/pieternortje/' target='_blank' style={{textDecoration: 'none' ,color:'white'}}><i className=\"fa fa-linkedin-square nav-i\"></i></a>\n                  <a href=\"mailto:pieter_nortje@outlook.com\" style={{color: \"white\"}}>{'pieter_nortje@outlook.com'}</a>\n                </div>\n      </div>\n      <div className=\"main-container\" style={{overflowY : 'scroll', height: '100vh'}}>\n        <div id='home_scroll'></div>\n        <Hero></Hero>\n        <div id=\"about_scroll\"></div>\n        <About></About>\n        <div id=\"work_scroll\"></div>\n        <History></History>\n        <div id='projects_scroll'></div>\n        <Projects></Projects>\n        <div id='contact_scroll'></div>\n        <Contact></Contact>\n      </div>\n    </div>\n  );\n}\n\nexport default App;\n","/Users/pieternortje/Documents/Development/Portfolio2-main/src/components/projects/index.jsx",["55","56"],"import {Container} from \"react-bootstrap\"\nimport {Center,Stack,Text,Divider} from '@chakra-ui/react'\nimport tutpro from \"../../assets/TUTPRO.png\"\nimport doterra from \"../../assets/DOTERRA.png\"\nimport smitten from \"../../assets/Smitten.png\"\nimport mibrary from \"../../assets/Mibrary.png\"\nimport solarbot from \"../../assets/TSPSolarBot.png\"\nimport blog from \"../../assets/Blog.png\"\nimport Card from './card'\nimport '../../style/projects.css';\n\nconst Projects = () => {\n    const projects = [\n    {\n        name: 'TSP Solar Bot',\n        description: 'A commercial monitoring dashboard for solar inverters.',\n        frontend: ['ReactJS', 'Chakra-ui', 'Data fetching / query caching', 'react-query'],\n        backend: ['GCP', 'Sys admin / DevOps', 'MySQL', 'Raspberry pi (collector)', 'Python flask API'],\n        links: [{name: 'live', url: 'https://tspsolarbot.com'}],\n        img: solarbot\n    },\n    {\n        name: 'Blog by dev_pieter',\n        description: 'My personal blog where I share my development experiences.',\n        frontend: ['ReactJS', 'Chakra-ui', 'Query caching and data fetcing with react-query)'],\n        backend: ['Digital Ocean', 'NodeJS API', 'Sys admin / DevOps', 'Nginx for domain routing', 'MongoDB'],\n        links: [{name: 'live', url: 'https://blog.devpieter.co.za'}, {name: 'code', url: 'https://github.com/dev-pieter'}],\n        img: blog\n    },\n    {\n        name: 'Mibrary',\n        description: 'A MERN Stack book library app.',\n        frontend: ['ReactJS', 'HTML/CSS', 'ReactJS hooks (State management)'],\n        backend: ['AWS', 'Sys-admin/DevOps', 'Nginx for domain routing', 'MongoDB'],\n        links: [{name: 'code', url: 'https://github.com/dev-pieter'}],\n        img: mibrary\n    },\n    {\n        name: 'Tutpro',\n        description: 'An online tutoring platform, built for students.',\n        frontend: ['C#', 'HTML/CSS'],\n        backend: ['Azure', 'Sys-admin/DevOps', '.NET MVC'],\n        links: [{name: 'code', url: 'https://github.com/dev-pieter'}],\n        img: tutpro\n    }]\n\n\n    return(\n        <section id=\"projects\">\n            <Container> \n                <Center height=\"200px\">\n                    <Divider width={'100%'}/>\n                </Center>\n                <Center>\n                    <Stack textAlign={'center'}>\n                        <h2 className=\"heading\" style={{color: \"white\"}}>| 03. Recent Work |</h2>\n                        <Text textColor=\"white\">\n                            Here are some examples of projects I have worked on in the past year of my code journey.<br/>\n                            <i>Ordered by most recent.</i>\n                        </Text>\n                    </Stack>\n                </Center>    \n            \n                <div className=\"projects-slider\">\n                    {projects.map(item => {\n                        return <Card details={item}></Card>\n                    })}\n                </div>   \n            </Container>\n        </section>\n    );\n};\n\nexport default Projects;","/Users/pieternortje/Documents/Development/Portfolio2-main/src/components/projects/card.jsx",["57","58","59"],"import React from 'react'\nimport {Box,HStack} from '@chakra-ui/react'\nimport { Text } from \"@chakra-ui/react\"\n\nexport default function card(props) {\n    return (\n        <Box xs={5} className=\"item\">\n            <img className=\"item-img\" src={props.details.img}></img>\n            <div className=\"hidden\">\n                <div>\n                    <h5><b>{props.details.name}</b></h5>\n                    <p>{props.details.description}</p>\n                    <HStack spacing='4'>\n                        <div>\n                            <p><b>Backend:</b></p>\n                            <list>\n                                {props.details.backend.map(item => \n                                    <li>{item}</li>\n                                )}\n                            </list>\n                        </div>\n                        <div>\n                            <p><b>Frontend:</b></p>\n                            <list>\n                                {props.details.frontend.map(item => {\n                                    return <li>{item}</li>\n                                })}\n                            </list>\n                        </div>\n                    </HStack>\n                    <br/>\n                </div>\n                \n                <HStack spacing={'5px'} textAlign={'center'}>\n                    {props.details.links.map(item => {\n                        return <a href={item.url} target=\"_blank\">{item.name}</a>\n                    })}\n                    \n                    {/* <a href=\"https://github.com/pietabrood/Mibrary\" target=\"_blank\"><b>Code</b></a> */}\n                </HStack>\n            </div>\n        </Box>\n    )\n}\n","/Users/pieternortje/Documents/Development/Portfolio2-main/src/components/work_history/history.jsx",["60","61","62"],"import React,  { useEffect, useState } from \"react\";\nimport { Divider, Stack, Center, Heading } from \"@chakra-ui/react\"\nimport {\n    Accordion,\n    AccordionItem,\n    AccordionButton,\n    AccordionPanel,\n    AccordionIcon,\n    Box,\n    Tag,\n    HStack\n  } from \"@chakra-ui/react\"\n\nconst History = () => {\n    return (\n        <section id=\"work\" >\n            <Stack minW={'70%'} maxW={'90%'}>\n            <h2 className=\"heading\" style={{color: \"white\"}}>| 02. Experience |</h2>\n            <Center>\n            <Accordion width={'100%'} textColor={'white'} defaultIndex={[0]} allowMultiple allowToggle>\n                <AccordionItem width={'100%'}>\n                    <Heading>\n                    <AccordionButton>\n                        <Box flex=\"1\" textAlign=\"left\">\n                        <HStack spacing=\"5\">\n                            <b>JavaScript/Python engineer<br/> at The Sun Pays</b><Tag variant={'outline'} color={'white'}>6 mon</Tag>\n                        </HStack>\n                        </Box>\n                        <AccordionIcon />\n                    </AccordionButton>\n                    </Heading>\n                    <AccordionPanel>\n                    I have been working at this fast-paced renewable energy company as a software engineer for almost 6 months. I have worked on a few exciting projects in my time there and learned a lot in terms of IOT, front-end and back-end development.\n                    <br/><br/>\n                    My skills as developer brought a lot to the table with the creation of a Solar Bot (Monitoring software for solar inverters and lithium batteries), helping clients extend the life of their solar setups. Here is a quick overview of the things I have worked on:\n                    <br/><br/>\n                    <ul>\n                        <li>Development and maintenance of Web application (Dashboard) built on React.js</li>\n                        <li>Development and integration of a Python flask API for data writing/fetching/updating to a MySQL database</li>\n                        <li>Deployment of production software to GCP (DevOps)</li>\n                        <li>IOT development on Raspberry pi, in Python, for data collection and communication with solar inverters. I used Modbus communication for reading/writing data to/from inverters and batteries.</li>\n                        <li>Goal oriented analysis of business outcomes with SDLC</li>\n                    </ul>\n                    </AccordionPanel>\n                </AccordionItem>\n                <AccordionItem width={'100%'}>\n                    <Heading>\n                    <AccordionButton>\n                    <Box flex=\"1\" textAlign=\"left\">\n                        <HStack spacing=\"5\">\n                            <b>Self employed web developer</b><Tag variant={'outline'} color={'white'}>1 yr</Tag>\n                        </HStack>\n                    </Box>\n                    <AccordionIcon />\n                    </AccordionButton>\n                    </Heading>\n                    <AccordionPanel>\n                    In my time at University, I took on a couple of side projects for private clients. I built and maintained websites/eCommerce stores and sought after the UI/UX design and feature integration to maximise these companies' reach and product delivery.\n                    <br/><br/>\n                    Technologies I used:\n                    <br/><br/>\n                    <ul>\n                        <li>JavaScript/HTML/CSS on the front-end</li>\n                        <li>PHP and SQL on the back-end</li>\n                        <li>Wordpress and WooCommerce for CMS/eCommerce</li>\n                    </ul>\n                    </AccordionPanel>\n                </AccordionItem>\n                \n            </Accordion>\n            </Center>\n            </Stack>\n        </section>\n    );\n};\n\nexport default History;",{"ruleId":"63","replacedBy":"64"},{"ruleId":"65","replacedBy":"66"},{"ruleId":"67","severity":1,"message":"68","line":1,"column":18,"nodeType":"69","messageId":"70","endLine":1,"endColumn":27},{"ruleId":"67","severity":1,"message":"71","line":1,"column":29,"nodeType":"69","messageId":"70","endLine":1,"endColumn":37},{"ruleId":"67","severity":1,"message":"72","line":2,"column":20,"nodeType":"69","messageId":"70","endLine":2,"endColumn":23},{"ruleId":"67","severity":1,"message":"73","line":2,"column":25,"nodeType":"69","messageId":"70","endLine":2,"endColumn":28},{"ruleId":"67","severity":1,"message":"74","line":2,"column":10,"nodeType":"69","messageId":"70","endLine":2,"endColumn":19},{"ruleId":"67","severity":1,"message":"73","line":1,"column":9,"nodeType":"69","messageId":"70","endLine":1,"endColumn":12},{"ruleId":"67","severity":1,"message":"72","line":1,"column":14,"nodeType":"69","messageId":"70","endLine":1,"endColumn":17},{"ruleId":"75","severity":1,"message":"76","line":11,"column":21,"nodeType":"77","endLine":11,"endColumn":125},{"ruleId":"75","severity":1,"message":"76","line":12,"column":21,"nodeType":"77","endLine":12,"endColumn":137},{"ruleId":"67","severity":1,"message":"78","line":6,"column":16,"nodeType":"69","messageId":"70","endLine":6,"endColumn":23},{"ruleId":"75","severity":1,"message":"76","line":34,"column":19,"nodeType":"77","endLine":34,"endColumn":123},{"ruleId":"75","severity":1,"message":"76","line":35,"column":19,"nodeType":"77","endLine":35,"endColumn":135},{"ruleId":"67","severity":1,"message":"79","line":4,"column":8,"nodeType":"6